                +crumbItemLink("/", "Home")
                +crumbDelimiter
                +crumbItemText("Accelerator")
            +messageHeader("Accelerator", "Holdex accelerator helps Web3 startups to overcome early-stage obstacles and grow big")()
            +short-links
                li: +short-link("/apply", "{Bolt}", "Apply Now")
                li: +short-link("/c/announcements/why-apply", "{InformationCircle}", "Why Apply")
                li: +short-link("/c/learn", "{BookOpen}", "Guides")
                li: +short-link("/about", "{UserGroup}", "About")
        +content-wrapper
            +filter-options
            +feedRenderer
