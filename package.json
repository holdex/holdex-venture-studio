{
  "name": "holdex-venture-studio",
  "version": "0.0.1",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "pre-dev": "node pull-env.js",
    "dev": "npm run pre-dev && npm run generate && vite dev",
    "clean-dev": "npm run generate && vite dev",
    "build": "npm run generate && vite build",
    "preview": "npm run generate  && vite preview",
    "generate": "graphql-codegen --config codegen.yml -r dotenv/config",
    "postinstall": "svelte-kit sync && sh ./apollo-client-patch.sh",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "npx eslint --ext js,ts,cjs,svelte --ignore-path .eslintignore --fix .",
    "format": "npx prettier --plugin-search-dir . --write ."
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@prettier/plugin-pug": "^3.4.0",
    "@steeze-ui/heroicons": "^2.4.2",
    "@sveltejs/adapter-auto": "^6.0.1",
    "@sveltejs/kit": "^2.27.1",
    "@types/lodash-es": "^4.17.12",
    "@types/numeral": "^2.0.5",
    "@types/rimraf": "^4.0.5",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prefer-arrow-functions": "^3.6.2",
    "eslint-plugin-svelte3": "^4.0.0",
    "form-data": "^4.0.4",
    "fs-extra": "^11.3.1",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "pug": "^3.0.3",
    "sass": "^1.90.0",
    "svelte": "^5.37.3",
    "svelte-check": "^4.3.1",
    "svelte-preprocess": "^6.0.3",
    "tailwindcss": "^4.1.11",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "vite": "^7.0.6"
  },
  "dependencies": {
    "@apollo/client": "^3.13.9",
    "@github/details-dialog-element": "^3.1.4",
    "@sendgrid/mail": "^8.1.5",
    "apexcharts": "^5.3.2",
    "dotenv": "^17.2.1",
    "editorjs-html": "^4.0.5",
    "graphql": "^16.11.0",
    "insane": "^2.6.2",
    "lodash-es": "^4.17.21",
    "moment": "^2.30.1",
    "node-fetch": "^3.3.2",
    "numeral": "^2.0.6",
    "open-graph-scraper": "^6.10.0",
    "rimraf": "^6.0.1",
    "rollbar": "^2.26.4"
  },
  "type": "module"
}
